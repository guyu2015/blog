

.last-wrapper{
  
  width : 100%;
  font-size: 15px;
}
.last{
  display:flex;
  flex-direction: row;
  height : 80px;
  width : 100%;
  /* background-color: pink; */
  padding:10px 15px ;
  border-top:1px solid #eee;
}
.pic-wrapper{
  flex : 1;
  display: inline-block;
  width:80px;
  height : 80px;

}
 .pic-wrapper image{
  width : 100%;
  height: 100%;
}
.text-wrapper{
  
  height : 80px;
  position:relative;
  flex: 3;
  padding:0 8px;
}
.last-right{
  margin-top:0;
  /* line-height: 20px; */
  display:block;
}
.left-small{
  margin-top: 50px;
  position:absolute;
  left : 15px;
  bottom : 10px;
  color:darkorange;
  font-size: 13px;
}
.right-small{
  position:absolute;
   right : 35px; 
  bottom : 10px;
  color:darkgray;
  font-size: 13px;
} 
.big{
  font-size: 30px;
  color:blueviolet;
  font-weight: bold;
}
.back{
  
  font-size: 15px;
  color:blueviolet;
  font-weight: bold;

}